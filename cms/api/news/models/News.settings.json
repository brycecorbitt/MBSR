{
  "connection": "default",
  "collectionName": "newsstories",
  "info": {
    "name": "news",
    "description": "News Stories to populate the \"Mindfulness Around The World\" section of the MBSR App."
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "link": {
      "type": "string",
      "required": true
    }
  }
}